Imports System.Console
Imports System.Math
Module Math1
    Sub Main()
        WriteLine("Sine 90 is" & " " & Sin(90))
        'display Sine90 value
        WriteLine("Square root of 81 is " & " " & Sqrt(81))
        'displays square root of 81
        WriteLine("Log value of 12 is" & " " & Log(12))
        WriteLine("Floor(6.99999) : " & Floor(6.99999))
        WriteLine("Ceiling(1.0000001) : " & Ceiling(1.0001))
        WriteLine("Pow(2,5) : " & Pow(2, 5))
        WriteLine("Truncate(5.65465) : " & Truncate(5.65465))

        'displays the logarithm value of 12
        Read()
    End Sub

End Module


